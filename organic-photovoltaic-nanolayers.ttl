###  https://w3id.org/emmo/application/organic-photovoltaic-nanolayers#AFMCalibrationProcess
:AFMCalibrationProcess rdf:type owl:Class ;
                       rdfs:subClassOf chameo:CalibrationProcess ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty emmo:EMMO_499e24a5_5072_4c83_8625_fe3f96ae4a8d ;
                                         owl:someValuesFrom :AFMMeasurementProcess
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty chameo:hasReferenceSample ;
                                         owl:someValuesFrom :CalibrationGratings
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty chameo:hasInstrumentForCalibration ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass :NtegraNtMDT
                                       ] ;
                       skos:prefLabel "AFMCalibrationProcess" ;
                       emmo:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9 "Scanning reference samples for calibration of the X-Y-Z motion+"@en .


